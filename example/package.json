{
  "name": "diplodoc-mdx-extension-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "run": "npm run build && npm run start",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,md}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,md}'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier",
    "lint:fix": "npm run lint:js:fix && npm run lint:styles:fix && npm run lint:prettier:fix && npm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@diplodoc/transform": "^4.57.3",
    "@gravity-ui/icons": "^2.13.0",
    "@gravity-ui/navigation": "^3.1.1",
    "@gravity-ui/uikit": "^7.4.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@swc/core": "^1.12.9",
    "bem-cn-lite": "^4.1.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "markdown-it": "^13.0.2",
    "mermaid": "^11.6.0",
    "next": "^15.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "threads": "^1.7.0"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.3.0",
    "@gravity-ui/eslint-config": "^3.2.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@svgr/webpack": "^8.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "stylelint": "^15.11.0",
    "typescript": "^5.7.2"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
